<Page x:Class="WPF1SemesterOpgave.Page2Overblik"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF1SemesterOpgave"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1Login">
    <Grid x:Name="Overblik">
        <Label x:Name="overblikTitel" HorizontalAlignment="Center" Background="LightGray" Margin="0,62,0,0" VerticalAlignment="Top" Width="422" RenderTransformOrigin="0.5,0.5" FontFamily="Bauhaus 93" FontSize="16" Height="25">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <TextBox x:Name="textOverblik" HorizontalAlignment="Left" Margin="370,65,0,0" TextWrapping="NoWrap" Text="OVERBLIK" VerticalAlignment="Top" Width="101" Background="LightGray" BorderThickness="0,0,0,0" FontSize="16"/>
        <Button x:Name="ButtonMedlemshåndtering" Content="Medlemshåndtering" HorizontalAlignment="Left" Margin="280,154,0,0" VerticalAlignment="Top" Width="245" ClickMode="Press" RenderTransformOrigin="0.5,0.5" Click="ButtonMedlemshåndtering_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.045"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="ButtonKursushåndtering" Content="Kursushåndtering" HorizontalAlignment="Left" Margin="280,225,0,0" VerticalAlignment="Top" Width="244" ClickMode="Press" RenderTransformOrigin="0.5,0.5" Click="ButtonKursushåndtering_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.173"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>


    </Grid>
</Page>
