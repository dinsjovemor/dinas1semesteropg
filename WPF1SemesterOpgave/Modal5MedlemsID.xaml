<Window x:Class="WPF1SemesterOpgave.Modal5MedlemsID"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF1SemesterOpgave"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Modal5MedlemsID">

    <Grid x:Name="mailadresseIndtastningsfelt">
        <Image x:Name="fotoMedlem1" HorizontalAlignment="Left" Height="50" Margin="611,73,0,0" VerticalAlignment="Top" Width="50" Source="/FOTOSKITSE.png"/>
        <TextBox x:Name="nameTextMedlem1" HorizontalAlignment="Left" Margin="611,128,0,0" TextWrapping="NoWrap" Text="Medlem1" VerticalAlignment="Top" Width="58" BorderThickness="0,0,0,0" FontSize="10"/>
        <Label x:Name="medlemsIDTitel" HorizontalAlignment="Center" Foreground="DarkRed" Background="LightGray" Margin="0,73,0,0" VerticalAlignment="Top" Width="422" RenderTransformOrigin="0.5,0.5" FontFamily="Bauhaus 93" FontSize="16" Height="25">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <TextBox x:Name="textMedlemmer" HorizontalAlignment="Left" Margin="360,75,0,0" TextWrapping="NoWrap" Text="MEDLEMSID" VerticalAlignment="Top" Width="120" Background="LightGray" BorderThickness="0,0,0,0" FontSize="16"/>
        <TextBox x:Name="navnIndtastningsfelt" HorizontalAlignment="Left" Margin="189,157,0,0" TextWrapping="Wrap" Text="Navn" VerticalAlignment="Top" Width="465"/>
        <TextBox x:Name="adresseIndtastningsfelt" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Adresse" VerticalAlignment="Top" Width="465" Margin="189,198,0,0"/>
        <TextBox x:Name="mailndtastningsfelt" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Mailadresse" VerticalAlignment="Top" Width="465" Margin="188,277,0,0"/>
        <TextBox x:Name="telefonIndtastningsfelt" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Telefonnummer" VerticalAlignment="Top" Width="465" Margin="189,237,0,0"/>
        <TextBox x:Name="tilmeldtlndtastningsfelt" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Tilmeld kursus" VerticalAlignment="Top" Width="52" Margin="189,332,0,0"/>
        <TextBox x:Name="indtastÆndringerneForklaring" HorizontalAlignment="Center" Margin="0,133,0,0" TextWrapping="NoWrap" Text="Indtast ændringerne direkte i felterne:" VerticalAlignment="Top" Width="422" Height="24" BorderBrush="White" Foreground="#FF161616" FontSize="10" Opacity="0.5"/>
        <TextBox x:Name="indtastÆndringerneForklaring_Copy" HorizontalAlignment="Left" Margin="500,295,0,0" TextWrapping="NoWrap" Text="Ændringerne gemmes automatisk" VerticalAlignment="Top" Width="153" Height="24" BorderBrush="White" Foreground="#FF161616" FontSize="10" Opacity="0.5"/>
        <Button x:Name="gemOgLukButton" Content="Gem og Luk" Margin="576,332,147,68" Click="gemOgLukButton_Click"/>

    </Grid>
</Window>
