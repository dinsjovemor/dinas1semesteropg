<Window x:Class="WPF1SemesterOpgave.Modal5MedlemsID"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF1SemesterOpgave"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Modal5MedlemsID">

    <Grid x:Name="mailadresseIndtastningsfelt">
        <Grid.RowDefinitions>
            <RowDefinition Height="0*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Image x:Name="fotoMedlem1" HorizontalAlignment="Left" Height="50" Margin="593,98,0,0" VerticalAlignment="Top" Width="50" Source="/FOTOSKITSE.png" Grid.RowSpan="2"/>
        <TextBox x:Name="textMedlemmer" HorizontalAlignment="Left" Margin="155,66,0,0" TextWrapping="NoWrap" TextAlignment="Center" Text="MEDLEMSID" VerticalAlignment="Top" Height="26" Width="500" Background="LightGray" BorderThickness="0,0,0,0" FontSize="16" Grid.RowSpan="2"/>
        <TextBox x:Name="navnIndtastningsfelt" HorizontalAlignment="Left" Margin="171,182,0,0" TextWrapping="Wrap" Text="Navn" VerticalAlignment="Top" Width="465" Grid.RowSpan="2"/>
        <TextBox x:Name="adresseIndtastningsfelt" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Adresse" VerticalAlignment="Top" Width="465" Margin="171,223,0,0" Grid.RowSpan="2"/>
        <TextBox x:Name="mailndtastningsfelt" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Mailadresse" VerticalAlignment="Top" Width="465" Margin="170,302,0,0" Grid.RowSpan="2"/>
        <TextBox x:Name="telefonIndtastningsfelt" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Telefonnummer" VerticalAlignment="Top" Width="465" Margin="171,262,0,0" Grid.RowSpan="2"/>
        <TextBox x:Name="indtastÆndringerneForklaring" HorizontalAlignment="Left" Margin="172,158,0,0" TextWrapping="NoWrap" Text="Indtast ændringerne direkte i felterne:" VerticalAlignment="Top" Width="422" Height="24" BorderBrush="White" Foreground="#FF161616" FontSize="10" Opacity="0.5" Grid.RowSpan="2"/>
        <TextBox x:Name="ÆndringerneForklaring" HorizontalAlignment="Left" Margin="482,320,0,0" TextWrapping="NoWrap" Text="Ændringerne gemmes automatisk" VerticalAlignment="Top" Width="153" Height="24" BorderBrush="White" Foreground="#FF161616" FontSize="10" Opacity="0.5" Grid.RowSpan="2"/>
        <Button x:Name="gemOgLukButton" Content="Gem og Luk" Height="35" Width="70" HorizontalAlignment="Left" Margin="558,357,0,42" Click="gemOgLukButton_Click" Grid.RowSpan="2"/>
        <Button x:Name="tilmeldKursusButton" Content="Tilmeld Kursus" Height="35" Width="80" HorizontalAlignment="Left" Margin="172,357,0,42" Click="tilmeldKursusButton_Click" Grid.RowSpan="2" d:LayoutOverrides="HorizontalAlignment" Background="#FFE8E8E8" ClickMode="Press"/>


    </Grid>
</Window>
