<Page x:Class="WPF1SemesterOpgave.Page1Login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF1SemesterOpgave"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1Login">
    <Grid>
        <Rectangle x:Name="BrugernavnIndtasningsfelt" HorizontalAlignment="Left" Height="26" Margin="304,154,0,0" Stroke="DarkGray" VerticalAlignment="Top" Width="204"/>
        <Rectangle x:Name="PasswordIndtastningsfelt" HorizontalAlignment="Left" Height="26" Margin="305,206,0,0" Stroke="DarkGray" VerticalAlignment="Top" Width="204"/>
        <Label x:Name="overblikTitel" HorizontalAlignment="Center" Background="LightGray" Margin="0,69,0,0" VerticalAlignment="Top" Width="422" RenderTransformOrigin="0.5,0.5" FontFamily="Bauhaus 93" FontSize="16" Height="25">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <TextBox x:Name="textDaniadata" HorizontalAlignment="Left" Margin="348,71,0,0" TextWrapping="NoWrap" Text="d a n i a d a t a" VerticalAlignment="Top" Width="116" Background="LightGray" BorderThickness="0,0,0,0" FontSize="16" TextChanged="textOverblik_TextChanged"/>
        <Button x:Name="loginButton" Content="Login" HorizontalAlignment="Left" Margin="476,251,0,0" VerticalAlignment="Top" Click="loginButton_Click"/>
        <TextBox x:Name="brugernavnUser" HorizontalAlignment="Left" Margin="317,158,0,0" TextWrapping="Wrap" Text="Brugernavn" VerticalAlignment="Top" Width="120" BorderBrush="White"/>
        <TextBox x:Name="passwordUser" HorizontalAlignment="Left" Margin="317,210,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="120" BorderBrush="White"/>


    </Grid>
</Page>
