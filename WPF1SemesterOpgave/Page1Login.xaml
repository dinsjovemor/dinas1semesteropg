<Page x:Class="WPF1SemesterOpgave.Page1Login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF1SemesterOpgave"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1Login">
    <Grid>
        <Rectangle x:Name="BrugernavnIndtasningsfelt" HorizontalAlignment="Left" Height="25" Margin="306,138,0,0" Stroke="DarkGray" VerticalAlignment="Top" Width="204"/>
        <Rectangle x:Name="PasswordIndtastningsfelt" HorizontalAlignment="Left" Height="25" Margin="307,190,0,0" Stroke="DarkGray" VerticalAlignment="Top" Width="204" d:LayoutOverrides="VerticalAlignment"/>
        <TextBox x:Name="textDaniadata" HorizontalAlignment="Left" Margin="155,66,0,0" TextWrapping="NoWrap" TextAlignment="Center" Text="d a n i a d a t a" VerticalAlignment="Top" Height="26" Width="500" Background="LightGray" BorderThickness="0,0,0,0" FontSize="16" TextChanged="textOverblik_TextChanged"/>
        <Button x:Name="loginButton" Content="Login" HorizontalAlignment="Left" Margin="478,235,0,0" VerticalAlignment="Top" Click="loginButton_Click"/>
        <TextBox x:Name="brugernavnUser" HorizontalAlignment="Left" Margin="319,142,0,0" TextWrapping="Wrap" Text="Brugernavn" VerticalAlignment="Top" BorderBrush="White"/>
        <TextBox x:Name="passwordUser" HorizontalAlignment="Left" Margin="319,194,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" BorderBrush="White" d:LayoutOverrides="VerticalAlignment"/>


    </Grid>
</Page>
