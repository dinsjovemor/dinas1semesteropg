<Page x:Class="dinasOpgaveDaniadata.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:dinasOpgaveDaniadata"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1Login">
    <Grid x:Name="Overblik">
        <Label x:Name="overblikTitel" HorizontalAlignment="Center" Background="LightGray" Margin="0,62,0,0" VerticalAlignment="Top" Width="422" RenderTransformOrigin="0.5,0.5" FontFamily="Bauhaus 93" FontSize="16" Height="25">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <TextBox x:Name="textOverblik" HorizontalAlignment="Left" Margin="370,66,0,0" TextWrapping="NoWrap" Text="OVERBLIK" VerticalAlignment="Top" Width="54" Background="LightGray" BorderThickness="0,0,0,0"/>
        <Button x:Name="ButtonMedlemshåndtering" Content="Medlemshåndtering" HorizontalAlignment="Left" Margin="189,154,0,0" VerticalAlignment="Top" Width="416"/>


    </Grid>
</Page>
