<Page x:Class="dinasOpgaveDaniadata.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:dinasOpgaveDaniadata"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1Login">
    <Grid>
        <Label x:Name="daniaDataLogin" HorizontalAlignment="Center" Foreground="DarkRed" Background="LightGray" Margin="0,62,0,0" VerticalAlignment="Top" Width="422" RenderTransformOrigin="0.5,0.5" FontFamily="Bauhaus 93" FontSize="16" Height="25">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Rectangle x:Name="BrugernavnIndtasningsfelt" HorizontalAlignment="Center" Height="26" Margin="0,175,0,0" Stroke="DarkGray" VerticalAlignment="Top" Width="422"/>
        <TextBlock x:Name="textBrugernavn" HorizontalAlignment="Left" Margin="189,154,0,0" TextWrapping="NoWrap" Text="Brugernavn" VerticalAlignment="Top" Width="179"/>
        <Rectangle x:Name="PasswordIndtastningsfelt" HorizontalAlignment="Center" Height="26" Margin="0,266,0,0" Stroke="DarkGray" VerticalAlignment="Top" Width="422"/>
        <TextBlock x:Name="textPassword" HorizontalAlignment="Left" Margin="189,245,0,0" TextWrapping="NoWrap" Text="Password" VerticalAlignment="Top" Width="179"/>
        <TextBox x:Name="textDaniadataLogin" HorizontalAlignment="Center" Margin="0,62,0,0" TextWrapping="NoWrap" Text="daniadata LOGIN" VerticalAlignment="Top" Width="120" Background="LightGray"/>


    </Grid>
</Page>
